        <!-- BEGIN LOGIN -->
        <div class="content">
            <!-- BEGIN LOGIN FORM -->
              <%= form_for(resource, :as => resource_name, :url => password_path(resource_name),:html => { :method => :post }, class:"login-form") do |f| %>
                <h3 class="form-title font-green"><%= t('.forgot_your_password', :default => 'Forgot your password?') %></h3>
                <div class="alert alert-danger display-hide">
                    <button class="close" data-close="alert"></button>
                    <span> Enter any username and password. </span>
                </div>
                <div class="form-group">
                     <%= f.password_field :email, class: 'form-control', placeholder: t(:email) %>
                </div>

                <div class="form-actions">
                  <%= f.submit t('.send_me_reset_password_instructions', :default => "Send me reset password instructions"), class: 'btn green uppercase' %>
                </div>
                <%= render "devise/shared/links" %>
            <% end  %>
